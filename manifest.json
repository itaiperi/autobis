{
  "name": "Autobis",
  "version": "0.1",
  "description": "Chrome extension for automating 10Bis for the better!",
  "icons": {
    "16": "resources/icons/icon_16.png",
    "48": "resources/icons/icon_48.png",
    "128": "resources/icons/icon_128.png"
  },
  "permissions": [
    "alarms",
    "tabs",
    "https://www.10bis.co.il/*",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "manifest_version": 2
}